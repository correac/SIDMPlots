#!/bin/bash
#SBATCH --job-name=AnalysisL25N752
#SBATCH --partition=thin
#SBATCH --time=36:00:00
#SBATCH --nodes=1
#SBATCH --tasks-per-node=1
#SBATCH --cpus-per-task=64
#SBATCH --hint=multithread
#SBATCH -o output-%j.out
